#############################################################################
# Makefile for building targets in sub directories.
# Generated by tmake at 15:24, 2001/10/29
#     Project: designer
#    Template: subdirs
#############################################################################

SUBDIRS =	util \
		uic \
		designer

all:
	$(MAKE) util
	$(MAKE) uic
	$(MAKE) designer


$(SUBDIRS): FORCE
	if [ -d $@ ]; then cd $@ ; $(MAKE); fi

clean:
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) $@; fi; ) ; done

FORCE:
