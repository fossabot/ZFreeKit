+#include <stack>		// STL
+template <typename T>
+class Q_EXPORT QValueStack
+{
+ private: std::stack<T> st;
+
+ public:
+   QValueStack() {}
+   ~QValueStack() {}
+
+   void  push( const T& d ) {
+      st.push(d);
+   }
+
+   // use QT semantics, hide std::stack
+   const T pop2() const {
+      const T d = st.top();
+      st.pop();
+      return d;
+   }
+   
+   // use QT semantics, hide std::stack
+   T pop2() {
+      T d = st.top();
+      st.pop();
+      return d;
+   }
+
+   T& top() { return st.top();}
+   const T& top() const { st.top(); }
+
+   bool isEmpty() const { return st.empty(); }
+
+   int count() const { return st.size(); }
+
+   void clear(){
+      while (!st.empty()) {
+	 st.pop();
+      }
+   }
+};
